page{
  background-color: #fff;
}
/*backgroundImg*/
.backgroundImg{
  width: 100%;
  height: 300rpx;
}
.backgroundImg image{
  width:100%;
  height: 100%;
}

/**clsUserHead*/
.clsUserHead{
  position:absolute;
  display: flex;
  flex-direction: row;
  top:0;
}
.clsUserHead navigator{
  margin:30rpx;
}
.clsUserHead image{
  width:170rpx;
  height: 170rpx;
  border-radius: 150rpx;
  margin:10rpx;
}
.clsName{
  display: flex;
  flex-direction: column;
  margin:30rpx;
}
.clsName image{
  margin-bottom: 1rpx;
}
.clsName label{
  text-align: center;
  padding-top: 8rpx;
  color: #fff;
}


/**clsUserInfo*/
.clsUserInfo{
  z-index: 999;
  width:100%;
  background-color: #fff;
}
.clsView{
  position:  relative;
  display:flex;
  flex-direction:row;
  padding: 30rpx 50rpx 20rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #dedede;
  /*margin-bottom:10rpx;*/
}
.clsView label{
  line-height: 80rpx;
  vertical-align: middle;
  margin-left: 30rpx;
  color: #1d1d26;
  font-size:30rpx;
}
.clsView .clsIcon{
  width:50rpx;
  height: 50rpx;
  margin: auto 0;
  display: block;
}
.clsView .clsEnterIcon{
  position: absolute;
  width:30rpx;
  height: 30rpx;
  display: block;
  right: 60rpx;
  top:37%;
}
.clsViewLast{
  border-bottom: none;
  margin-bottom:0;
}

/*clsBarBackgroundImg*/
.clsBarBackgroundImg{
  width: 100%;
  height:100rpx;
}
.clsBarBackgroundImg image{
  clear:both;
  width:100%;
  height: 100%;
}