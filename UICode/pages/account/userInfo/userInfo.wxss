page{
  background-color: #fff;
}
.containePlanner{
  background-color: #f0f0f0;
  padding: 20rpx 0;
}
/*backgroundImg*/
.backgroundImg{
  width: 100%;
  height: 300rpx;
}
.backgroundImg image{
  width:100%;
  height: 100%;
}

/**clsUserHead*/
.clsUserHead{
  position:absolute;
  display: flex;
  flex-direction: row;
  top:0;
}
.clsUserHead navigator{
  margin:30rpx;
}
.clsUserHead image{
  width:170rpx;
  height: 170rpx;
  border-radius: 150rpx;
  margin:10rpx;
}
.clsName{
  display: flex;
  flex-direction: column;
  margin:30rpx;
}
.clsName image{
  margin-bottom: 1rpx;
}
.clsName label{
  text-align: center;
  padding-top: 8rpx;
  color: #fff;
  width: 100%;
  overflow: hidden;
  font-size: 30rpx;
}

.clsPlannerHead{
  margin: 20rpx;
  margin-top:0;
  padding: 30rpx;
  background-color: #FFF;
  border-radius: 8rpx;
}
.clsPlannerHead image{
  width:150rpx;
  height: 150rpx;
  border-radius: 150rpx;
  margin:10rpx;
}
.clsPlannerHead label{
  padding-left: 18rpx;
  line-height: 170rpx;
  vertical-align: middle;
}
.clsPlannerHead navigator{
  /* margin-left: 80rpx;  */
  color: #099aff;
  border:2rpx solid #099aff;
  border-radius: 8rpx;
  padding: 10rpx 30rpx;
  height: 36rpx;
  font-family: "黑体";
  font-size: 26rpx;
  margin:auto 0;
  margin-left: auto;
}

/**clsUserInfo*/
.clsUserInfo{
  z-index: 999;
  width:100%;
  background-color: #fff;
}
.clsUserInfoPlanner{
  margin: 20rpx;
  width:auto;
  border-radius: 8rpx;
}
.clsView{
  position:  relative;
  display:flex;
  flex-direction:row;
  padding: 30rpx 50rpx 20rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #dedede;
  /*margin-bottom:10rpx;*/
}
.clsView label{
  line-height: 80rpx;
  vertical-align: middle;
  margin-left: 30rpx;
  color: #1d1d26;
  font-size:30rpx;
}
.clsView .clsIcon{
  width:50rpx;
  height: 50rpx;
  margin: auto 0;
  display: block;
}
.clsView .clsEnterIcon{
  position: absolute;
  width:30rpx;
  height: 30rpx;
  display: block;
  right: 60rpx;
  top:37%;
}
.clsViewLast{
  border-bottom: none;
  margin-bottom:0;
}

/*clsBarBackgroundImg*/
.clsBarBackgroundImg{
  width: 100%;
  height:100rpx;
}
.clsBarBackgroundImg image{
  clear:both;
  width:100%;
  height: 100%;
}