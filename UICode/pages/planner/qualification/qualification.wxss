.container{
  margin: 20rpx;
  font-size: 30rpx;
  margin-bottom: 35rpx;
}

/**plannerInfo*/
.plannerInfo{
  display: flex;
  flex-direction:row;
  background-color: #fff;
  padding:25rpx;
  border-radius: 8rpx;
}
.plannerImg{
  width: 150rpx;
  height: 100%;
}
.plannerImg .clsHead{
  width:130rpx;
  height: 130rpx;
  border-radius: 160rpx;
  margin:10rpx;
}
.plannerHeadTxt{
  width: 100%;
  position: relative;
}
.plannerBtnMsg{
  position: absolute;
  right: 0;
  top: 42rpx;
  margin: 10rpx;
}
.plannerBtnMsg image{
  width: 120rpx;
  height: 60rpx;
}
.clsPlannerTxt{
  display: flex;
  flex-direction:row;
  
  margin: 12rpx;
  color: #3e3e3e;
}
.clsTeamTxt{
  color: #929292;
}
.clcComment{
  color: #099aff;
}
.clsBtn02{
  width: 140rpx;
  background-color: #099aff;
  color: #fff;
  line-height:1.8;
  font-size:25rpx;
  margin-top: 15rpx;
  font-family: "黑体";
  border: none;
  border-radius:28rpx;
}

/*clsQualification*/
.clsQualifications{
  background-color: #fff;
  border-radius: 8rpx;
  padding:30rpx;
  margin-top: 20rpx;
}
.clsQualification{
  border-bottom:2rpx solid #eee;
  margin-top: 15rpx;
  padding: 15rpx 0;
}
.clsEducationTitle, .clsSocietyTitle, .clsResourTitle, .clsPersonTitle,.clsAutographTitle{
  color: #3e3e3e;
  margin: 15rpx 0;
  font-size: 38rpx;
  font-weight: bold;
  font-family: "黑体";
}
.clsEducation, .clsSociety, .clsResour,.clsPerson, .clsAutograph{
  color: #434340;
  margin-top: 10rpx;
}
.clsAutographHead{
  border-bottom:none;
}

.clsPlannerAreaService{
  display: flex;
  flex-direction:column;
  margin: 12rpx;
  color: #3e3e3e;
}
.clsPlannerArea{
  font-family: "黑体";
  display: flex;
  flex-wrap:wrap;
  width: 400rpx;
  line-height: 1.5;
}
.clsServiceLable{
  color:#fff;
  font-size:26rpx;
  padding:14rpx;
  vertical-align:middle;
  line-height:20rpx;
  margin-right:6rpx;
  margin-top: 5rpx;
}
.clsPlannerAreaService{
  display: flex;
  flex-direction:column;
  margin: 12rpx;
  color: #3e3e3e;
}
.clsPlannerTxtServer{
  font-family: "黑体";
  display: flex;
  flex-wrap:wrap;
  line-height: 1.5;
}

.clsLablesColor01{
  background-color: #afdd86;
}
.clsLablesColor02{
  background-color: #f6846a;
}
.clsLablesColor03{
  background-color: #67b0d6;
}
.clsLablesColor04{
  background-color: #dddd86;
}
.clsLablesColor05{
  background-color: #e5846a;
}
.clsLablesColor06{
  background-color: #56b0d6;
}
.clsLablesColor07{
  background-color: #67afd6;
}
.plannerName{
  text-align: center;
  line-height:1.8;
  font-size:25rpx;
  margin-top: 15rpx;
  font-family: "黑体";
}