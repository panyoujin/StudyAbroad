/**searchResult*/
.searchResult{
  margin: 20rpx;
  border-radius: 3px;
}
.searchResult .plannerInfo{
  display: flex;
  flex-direction:row;
  background-color: #FFF;
  margin-bottom: 4rpx;
  padding : 20rpx 20rpx 30rpx;
  position: relative;
}
.img{
  vertical-align: middle;
  padding:45rpx 35rpx 25rpx 25rpx;
}
.img image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 150px;
  border: 1px solid #eee;
}
.plannerTxtInfo{
  padding-top: 12rpx;
}
.plannerTxtInfo>view{
  margin-top:10rpx;
}
.clsName{
  font-size:35rpx;
}
.clsService{
  color: #099aff;
  font-size:26rpx;
}
.clsNewEvaluate{
  color: #727272;
  font-size:26rpx;
}
.clsLables{
  display: flex;
  flex-direction: row;
}
.clsLable{
  color:#fff;
  font-size:26rpx;
  padding:10rpx 20rpx;
  vertical-align:middle;
  line-height:42rpx;
  height:42rpx;
  margin-right:10rpx;

}
.clsLablesColor01{
  background-color: #ffdd86;
}
.clsLablesColor02{
  background-color: #f6846a;
}
.clsLablesColor03{
  background-color: #67b0d6;
}
.clsTeamName{
  position:absolute;
  right:30rpx;
  top:50%;
  color:#727272;
  font-size:26rpx;
  text-align: right;
}